<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Background Colors-->
    <Color x:Key="primaryBackColor1">#FFFFFF</Color>
    <Color x:Key="primaryBackColor2">#F8F9FA</Color>
    <Color x:Key="secondaryBackColor1">#F1F3F5</Color>
    <Color x:Key="secondaryBackColor2">#E9ECEF</Color>

    <!--Window Border Colors-->
    <Color x:Key="winBorderColor1">#773DFF</Color>
    <Color x:Key="winBorderColor2">#836EFB</Color>
    <Color x:Key="winBorderColor3">#6D2FFF</Color>

    <!--Color Palette-->
    <SolidColorBrush x:Key="color1" Color="#773DFF"/>
    <SolidColorBrush x:Key="color2" Color="#836EFB"/>
    <SolidColorBrush x:Key="color3" Color="#6D2FFF"/>
    <SolidColorBrush x:Key="color4" Color="#5727CC"/>
    <SolidColorBrush x:Key="color5" Color="#4A21B0"/>
    <SolidColorBrush x:Key="color6" Color="#3D1B94"/>
    <SolidColorBrush x:Key="color7" Color="#301578"/>
    <SolidColorBrush x:Key="color8" Color="#230F5C"/>
    <SolidColorBrush x:Key="color9" Color="#160940"/>
    <SolidColorBrush x:Key="color10" Color="#090324"/>
    <SolidColorBrush x:Key="color11" Color="#FFFFFF"/>

    <!--Font Colors-->
    <SolidColorBrush x:Key="titleColor1" Color="#212529"/>
    <SolidColorBrush x:Key="titleColor2" Color="#343A40"/>
    <SolidColorBrush x:Key="titleColor3" Color="#495057"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#495057"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="#6C757D"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="#ADB5BD"/>

    <!--Panel Colors-->
    <SolidColorBrush x:Key="panelColor" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#F8F9FA"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#E9ECEF"/>
    
    <!-- Additional Panel Brushes -->
    <SolidColorBrush x:Key="PanelBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="SecondaryBackgroundBrush" Color="#F8F9FA"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#DEE2E6"/>

    <!--Button Colors-->
    <!--TextBox Colors-->
    <!--Etc-->

</ResourceDictionary>